module: github.com/theNorstroem/FuroBaseSpecs
version: "v1.10.2"
typeSpecDir: "./Messages"
serviceSpecDir: "./Services"
specFormat: "yaml" #set to yaml or json
dependencies: [] #no dependencies :-)
dependenciesDir: installedSpecs
muSpec:
  types: []
  services: []
commands:
  publishNpm: "scripts/test.sh"
  buildPb: "scripts/buildPb.sh"
flows:
  default:
    - genMessageProtos
    - buildPb
    - genEsModule
build:
  proto:
    targetTypeDir: "./dist/proto/Messages" #Hint: add this to your proto include path
    targetServiceDir: "./dist/proto/Services" #Hint: add this to your proto include path
  esModule:
    targetFile: "./dist/es6/env.js"